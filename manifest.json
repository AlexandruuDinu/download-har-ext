{
    "manifest_version": 3,
    "name": "HAR Downloader",
    "version": "1.1",
    "description": "Automatically download HAR from a website with UI",
    "permissions": [
        "debugger",
        "activeTab",
        "downloads"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_title": "Capture HAR"
    }
}